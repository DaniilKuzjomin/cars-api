{
    "openapi": "3.0.2",
    "info": {
        "title": "Cars-API",
        "version": "1.0.0",
        "description": "### This is a Swagger UI for our Cars API"
    },
    "paths": {
        "/cars": {
            "get": {
                "tags": [
                    "cars"
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "examples": {
                                    "List of cars": {
                                        "value": [
                                            "BMW",
                                            "M3"
                                        ]
                                    }
                                }
                            }
                        }
                    }
                },
                "summary": "Get a list of all cars",
                "description": "Returns a list of all cars in API"
            }
        }
    }
}